<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="680.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Screen.App">
   <children>
      <ImageView fx:id="imageView" fitHeight="688.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Image/AppScreen.png" />
         </image></ImageView>
      <Button fx:id="buttonAW" layoutX="427.0" layoutY="20.0" mnemonicParsing="false" onAction="#switchtoAddWords" opacity="0.0" prefHeight="70.0" prefWidth="77.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="buttonSAW" layoutX="527.0" layoutY="151.0" mnemonicParsing="false" onAction="#switchtoShowAllWords" opacity="0.0" prefHeight="70.0" prefWidth="77.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="buttonSW" layoutX="559.0" layoutY="312.0" mnemonicParsing="false" onAction="#switchtoSearchWords" opacity="0.0" prefHeight="70.0" prefWidth="77.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="buttonTrans" layoutX="528.0" layoutY="467.0" mnemonicParsing="false" onAction="#switchtoTranslate" opacity="0.0" prefHeight="70.0" prefWidth="77.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="gamebutton" layoutX="453.0" layoutY="593.0" mnemonicParsing="false" opacity="0.0" prefHeight="70.0" prefWidth="77.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="buttonOut" layoutX="30.0" layoutY="578.0" mnemonicParsing="false" onAction="#handleButtonOutAction" opacity="0.0" prefHeight="83.0" prefWidth="84.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
